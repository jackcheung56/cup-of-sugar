=\"/items/delete/:item_id\"\n            render={(props) => <DeleteItemPage {...props} />}\n          />\n\n          <Route\n            exact\n            path=\"/items/update/:item_id\"\n            render={(props) => <EditItemPage {...props} />}\n          />\n          <Route\n            path=\"/login\"\n            component={(props) => (\n              <SignIn\n                setAuthenticated={setAuthenticated}\n                toggleAuthenticated={toggleAuthenticated}\n                user={user}\n                email={email}\n                setCurrentUser={setCurrentUser}\n                password={password}\n                setEmail={setEmail}\n                setPassword={setPassword}\n                setUser={setUser}\n                history={history}\n                {...props}\n              ></SignIn>\n            )}\n          />\n\n          <Route\n            path=\"/signup\"\n            component={(props) => (\n              <SignUp user={user} setUser={setUser}></SignUp>\n            )}\n          />\n\n          <Route\n            exact\n            path=\"/items/:item_id\"\n            render={(props) => (\n              <ItemDetailsPage\n                location={props.location}\n                currentUser={currentUser}\n                history={history}\n              ></ItemDetailsPage>\n            )}\n          />\n        </Switch>\n      )}\n    </div>\n  );\n}\n\nexport default Router;\n","/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/components/NavBar.js",[],"/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/pages/SignIn.js",["129"],"import React, { useState, useEffect } from \"react\";\nimport { __LoginUser } from \"../services/UserService\";\n<<<<<<< HEAD\n// import PropTypes from \"prop-types\";\n=======\nimport '../styles/Form.css'\n>>>>>>> e8af2464ea84b367edb82562e38520bc9e45b5f8\n\nconst SignIn = (props) => {\n  const [tempEmail, setTempEmail] = useState(\"\");\n  const [tempPassword, setTem