henticated(true, setUser.user, () =>\n        history.push(\"/users/:user_id\")\n      );\n    } catch (error) {\n      throw error;\n    }\n  };\n\n  return (\n    <form>\n      <div className=\"block\">\n        <input\n          placeholder=\"Enter Email\"\n          value={email}\n          onChange={emailInput}\n          type=\"text\"\n        ></input>\n        <input\n          placeholder=\"Enter Password\"\n          value={password}\n          onChange={passwordInput}\n          type=\"text\"\n        ></input>\n        <button onClick={loginHandler}>Login</button>\n      </div>\n    </form>\n  );\n};\n\nexport default SignIn;\n","/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/pages/SignUp.js",[],"/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/services/ItemService.js",[],"/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/pages/Home.js",["105","106"],"import React, { useState, useEffect } from \"react\";\n\nfunction Home({item, setItem}) {\n\n\n\n    useEffect(() => {\n        setItem(item)\n        console.log('state', item)\n      }, [])\n\n\n\n\n    return (\n        <div>\n            <h1>home</h1>\n        </div>\n    );\n}\n\nexport default Home;\n","/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/pages/Profile.js",[],"/home/lewist13/ga/Unit3/Week9/Projects/cup-of-sugar/client/src/pages/BrowsePage.js",["107","108"],"import React, { useState, useEffect } from \"react\";\nimport { useHistory } from 'react-router-dom';\nimport ItemCard from '../components/ItemCard'\n\nfunction BrowsePage(props) {\n    //Check to see if the data is being passed as props\n\n    // useEffect(() => {\n    //     setItem(item)\n    //     console.log('Check State', item)\n    //   }, [])\n\n    console.log(props)\n    \n    const itemList = props.item\n    const history = useHistory()\n    //Fun fact: this is the same as writing \"const history = props.history\" (i think...)\n    \n\n    return (\n        <div>\n   