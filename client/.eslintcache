 <button\n            className=\"conTags\"\n            type=\"button\"\n            placeholder=\"Acceptable\"\n            name=\"Acceptable\"\n            onClick={() => setCondition('Acceptable')}\n          >Acceptable</button>\n          <button\n            className=\"conTags\"\n            type=\"button\"\n            placeholder=\"Bad\"\n            name=\"Bad\"\n            onClick={() => setCondition('Bad')}\n          >Bad</button>\n        </div>\n        <input type=\"hidden\" name=\"condition\" value={newItem.condition = condition}></input>\n        <input type=\"hidden\" name=\"ownerId\" value={newItem.owner_id = loggedUser}></input>\n        <input type=\"hidden\" name=\"condition\" value={newItem.isBorrowed = false}></input>\n        <button className=\"styleButton\" type=\"submit\" onClick={handleSubmit}>Submit</button>\n      </form>\n    </div>\n  )\n}\nexport default AddItemPage\n","/Users/williamlong/general-assembly/unit3/projects/cup-of-sugar/client/src/pages/UserList.js",[],"/Users/williamlong/general-assembly/unit3/projects/cup-of-sugar/client/src/pages/Home.js",["135"],"import React, { useEffect } from \"react\";\nimport \"../styles/Home.css\";\n\nfunction Home({ item, setItem }) {\n  useEffect(() => {\n    setItem(item);\n  }, []);\n\n  return (\n    <div>\n      <h1>home</h1>\n    </div>\n  );\n}\n\nexport default Home;\n","/Users/williamlong/general-assembly/unit3/projects/cup-of-sugar/client/src/pages/LandingPage.js",["136"],"import React from \"react\";\nimport \"../styles/Landing.css\";\nimport Logo from \"../assets/coslogo.png\";\nimport { Link } from \"react-router-dom\";\n\nexport default () => {\n  return (\n    <div className=\"branding\">\n      <div className=\"lifted\">\n        <div className=\"object\">\n          <img src={Logo} alt=\"app logo\"></img>\n          <div className=\"welcome\">\n            <h2>cup of sugar</h2>\n          </div>\n        </div>\n\n        <div className=\"greeting\">\n          <Link to=\"