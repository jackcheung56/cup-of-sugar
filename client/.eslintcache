\n      console.log(error);\n    }\n  };\n\n  return (\n    <div className=\"requestBox\">\n      {!layout ? (\n        <div className=\"grid-container\">\n          <div className=\"container\">\n            <div className=\"iCon\">\n              <div className=\"circle\">\n                <p className=\"num\">{id}</p>\n              </div>\n            </div>\n            <ul className=\"list\">\n              <li className=\"liOne\">{product}</li>\n              <li className=\"liTwo\">Duration: {duration}</li>\n              <li className=\"liThree\">Contact: {message}</li>\n              <div className=\"ticketBtns\">\n                <button\n                  placeholder=\"Accept\"\n                  className=\"btnOne\"\n                  onClick={handleFillout}\n                >\n                  Accept\n                </button>\n                <button\n                  placeholder=\"Decline\"\n                  className=\"btnTwo\"\n                  onClick={deleteRequest}\n                >\n                  Decline\n                </button>\n              </div>\n            </ul>\n          </div>\n        </div>\n      ) : (\n        <div className=\"grid-containerB\">\n          {!confirmation ? (\n            <div className=\"containerB\">\n              <div className=\"ticketBtnsB\">\n                <button\n                  className=\"btnOneB\"\n                  onClick={(event) => {\n                    handleApproval(event);\n                  }}\n                >\n                  Confirm\n                </button>\n                <button className=\"btnTwoB\" onClick={deleteRequest}>\n                  Decline\n                </button>\n              </div>\n              <div className=\"formBox\">\n                <div className=\"cardName\">\n                  <p className=\"reply\">Message</p>\n                </div>\n                <input\n                  className=\"textMsg\"\n                  placeholder=\"message f